{
    "name": "api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "concurrently \"rm -rf dist\" \"npm run swagger\" ",
        "build": "npx tsc",
        "start": "node dist/index.js",
        "start-dev": "nodemon index.ts",
        "predev": "npm run swagger",
        "dev": "concurrently \"nodemon index.ts\" \"nodemon -x tsoa spec\" ",
        "swagger": "tsoa spec",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "nodemonConfig": {
        "ext": "ts",
        "exec": "ts-node index.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/lodash": "^4.14.202",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "lodash": "^4.17.21",
        "mongodb": "^6.2.0",
        "mongoose": "^7.6.3",
        "morgan": "^1.10.0",
        "nodemon": "^3.0.1",
        "swagger-ui-express": "^5.0.0",
        "ts-node": "^10.9.1",
        "tsoa": "^5.1.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.20",
        "@types/morgan": "^1.9.7",
        "@types/swagger-ui-express": "^4.1.5",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "prettier": "^3.0.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2",
        "rpg-app-shared-package": "1.0.0"
    }
}
